<!doctype html>
<html>

<head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <link rel="icon" type="image/x-icon" href="/images/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Oswald&family=Work+Sans&display=swap"
        rel="stylesheet">
    <meta name="viewport" content="width=device-width,initial-scale=1, shrink-to-fit=no">
    {{ content_for_header }}
    <link rel="preload" as="style" href="{{ 'style.global.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.announcement-bar.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.best-seller.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.cart.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.collection-master-content.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.collection-master-hero.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.featured-collections.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.image-banner.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.product.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.template-collection-list.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.header.css' | asset_url }}">
    <link rel="preload" as="style" href="{{ 'style.footer.css' | asset_url }}">

    {{ 'style.global.css' | asset_url | stylesheet_tag }}
    {{ 'style.announcement-bar.css' | asset_url | stylesheet_tag }}
    {{ 'style.best-seller.css' | asset_url | stylesheet_tag }}
    {{ 'style.cart.css' | asset_url | stylesheet_tag }}
    {{ 'style.collection-master-content.css' | asset_url | stylesheet_tag }}
    {{ 'style.collection-master-hero.css' | asset_url | stylesheet_tag }}
    {{ 'style.featured-collections.css' | asset_url | stylesheet_tag }}
    {{ 'style.image-banner.css' | asset_url | stylesheet_tag }}
    {{ 'style.product.css' | asset_url | stylesheet_tag }}
    {{ 'style.template-collection-list.css' | asset_url | stylesheet_tag }}
    {{ 'style.header.css' | asset_url | stylesheet_tag }}
    {{ 'style.footer.css' | asset_url | stylesheet_tag }}

    <script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js" defer></script>
    <script src="{{ 'script.functions.js' | asset_url }}" type="text/javascript" defer></script>
    <script src="{{ 'script.global.js' | asset_url }}" type="text/javascript" defer></script>
    <script src="{{ 'script.product.js' | asset_url }}" type="text/javascript" defer></script>
    <script src="{{ 'script.cart.js' | asset_url }}" type="text/javascript" defer></script>
    <script src="{{ 'script.scroll.js' | asset_url }}" type="text/javascript" defer></script>
</head>

<body class="template-{{ template | handleize }}">
    {% section 'announcement-bar' %}
    {% section 'header' %}

    {% if shop.customer_accounts_enabled %}
    {% if customer %}
    <a href="/account">account</a>
    {{ 'log out' | customer_logout_link }}
    {% else %}
    {{ 'log in ' | customer_login_link }}
    {{ 'register' | customer_register_link }}
    {% endif %}
    {% endif %}

    <main role="main">
        {{ content_for_layout }}
    </main>

    {% section 'footer' %}
</body>

</html>